<!DOCTYPE html>
<html>
  <head>
    <title>My_blog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="wrapper">
      <div id=header>
        <strong>RailsBlog</strong>
        <hr>
      </div>
      <div id="content">
      <%= yield %>
      </div>
    </div>
      
  </body>
</html>
